{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Open current HTML (file)",
      "type": "chrome",
      "request": "launch",
      "file": "${file}"
    },
    {
      "name": "Launch Chrome on http://localhost:5500/contact.html",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:5500/contact.html",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Launch Chrome on http://localhost:5500/breakfast.html",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:5500/breakfast.html",
      "webRoot": "${workspaceFolder}"
    }
  ]
}
